generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ads {
  id      Int     @id @default(autoincrement())
  picture String?
}

model news {
  id          Int     @id @default(autoincrement())
  title       String
  picture     String
  description String
  date        DateTime
}

model products {
  id          Int     @id @default(autoincrement())
  name        String
  picture     String
  description String
}